{
  "name": "zooid-button",
  "version": "1.2.3",
  "description": "Button",
  "main": "dist/bundle.min.js",
  "scripts": {
    "build": "webpack",
    "build:storybook": "build-storybook -o storybook-build",
    "clean": "rimraf lib dist coverage",
    "coverage": "nyc report --reporter=lcov",
    "lint": "eslint src stories",
    "lint:fix": "eslint src stories --fix",
    "start": "start-storybook -p 9001",
    "test": "nyc --require babel-core/register --require babel-polyfill mocha",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/zooid-button.git"
  },
  "keywords": [
    "zooid",
    "zooid-ui",
    "react",
    "reactjs",
    "octoblu"
  ],
  "author": "Octoblu Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/zooid-button/issues"
  },
  "homepage": "https://github.com/octoblu/zooid-button#readme",
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/**/*.css"
    ]
  },
  "babel": {
    "presets": [
      "latest",
      "react"
    ],
    "plugins": [
      [
        "styled-components"
      ]
    ]
  },
  "dependencies": {
    "styled-components": "2.0.0-10"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-styled-components": "^1.1.1",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "eslint": "^3.18.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "webpack": "^2.3.2"
  }
}
